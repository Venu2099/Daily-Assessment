package Practiceprogram09;

public class Arrprog1 {
	public static void main(java.lang.String args) {
		 int sum=0;
         int n[ ]=new int [10];
         for(int i=0;i<5;i++)
         {    n[i]=Integer.parseInt(args[i]);
             sum=sum+n[i];
         }
         int Total;
		System.out.println("Total=" + sum);
	}

}
